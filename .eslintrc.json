{
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "env": {
    "jest": true
  },
  "rules": {
    "import/no-duplicates": ["error"],
    "import/no-cycle": [
      "error",
      {
        "ignoreExternal": true
      }
    ],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
